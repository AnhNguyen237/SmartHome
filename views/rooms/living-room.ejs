<div class='right-container'>
    <div class='room-detail'>
        <div class='title-room'>
            <div class='room-icon'>
                <i class='fa-solid fa-couch'></i>
            </div>
            <div id='roomName' class='room-name'>
                Phòng Khách
            </div>
        </div>
        <div class='date-time'>
            <div id='current-time' class='text-content'></div>
            <div class='basic-info'>
                <div class='temperature'>
                    <div>
                        <h1 class='text-value temp'><%=data.temp%>&degC</h1>
                    </div>
                </div>
                <div class='space-column'></div>
                <div class='humidity'>
                    <div>
                        <h1 class='text-value humd'><%=data.humd%>&#37</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class='space-row'></div>

    <div class='room-control'>
        <div class='control-title'>
            <h1 class='control-title-content'>
                Hiệu Chỉnh Thiết Bị
            </h1>
        </div>
