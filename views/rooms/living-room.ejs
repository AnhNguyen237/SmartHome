                            <a href='#'>
                                <h2 class='quipment-name'>Điều hòa</h2>
                            </a>
                        </div>
                    </div>
                    <div class='basic-function'>
                        <div class='remote-air-fan'>
                            <h2 class='air-fan-name'>Tốc độ</h2>
                            <div class='buttons_added'>
                                <input class='minus is-form post' type='button' value='-'>
                                    <input id="airSpeedFan-liv" aria-label='quantity' class='input-qty' max='5' min='0' name='speed-fan-air' type='number' value='<%=roomDevice[1].speedFanAir%>'>
                                        <input class='plus is-form post' type='button' value='+'>
                            </div>
                        </div>
                        <div class='remote-air-temperature'>
                            <h2 class='change-temperature'>Nhiệt độ</h2>
                            <div class='buttons_added'>
                                <input class='minus is-form post' type='button' value='-'>
                                    <input id="airTemp-liv" aria-label='quantity' class='input-qty' max='35' min='10' name='' type='number' value='<%=roomDevice[1].tempAir%>'>
                                        <input class='plus is-form post' type='button' value='+'>
                                    </div>
                        </div>
                    </div>
                </div>
            </div>
