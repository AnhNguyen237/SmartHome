                        <div class='status'>
                            <h1 class='equipment-status-title'>Trạng thái</h1>
                            <div class='button r post' id='button-1'>
                                <% if(roomDevice[1].status) { %>
                                    <input id="airStatus-liv" type='checkbox' class='checkbox'/>
                                <% }else { %>
                                    <input id="airStatus-liv" type='checkbox' class='checkbox' checked/>
