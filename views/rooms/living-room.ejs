            <div class='equipment-row'>
                <div class='equipment lamp'>
                    <div class='equipment-status'>
                        <div class='status'>
                            <h1 class='equipment-status-title'>Trạng thái</h1>
                            <div class='button r post' id='button-1'>
                                <% if(roomDevice[0].status) { %>
                                <input id="lampStatus-liv" type='checkbox' class='checkbox'/>
                                <% }else { %>
                                <input id="lampStatus-liv" type='checkbox' class='checkbox' checked/>
                                <% } %>
                                <div class='knobs'></div>
                                <div class='layer'></div>
                            </div>
                        </div>
                        <div class='shims' style='flex:1'></div>
                        <div class='auto-mode'>
                            <h1 class='equipment-status-title'>Tự động</h1>
                            <div class='button r post' id='button-1'>
                                <% if(roomDevice[0].auto) { %>
                                <input id="lampAuto-liv" type='checkbox' class='checkbox'/>
                                <% }else { %>
                                <input id="lampAuto-liv" type='checkbox' class='checkbox' checked/>
                                <% } %>
                                <div class='knobs'></div>
                                <div class='layer'></div>
                            </div>
                        </div>
                    </div>
                    <div class='equipment-img'>
                        <div class='images lamp-img'></div>
                        <div class='to-remote'>
                            <a href='#'>
                                <h2 class='quipment-name'>Đèn</h2>
                            </a>
                        </div>
                    </div>
                </div>
